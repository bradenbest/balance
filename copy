#!/bin/bash
# Rather than fight with make, I'm using bash for this part

if [ -f ~/.balance_data ] ; then 
  echo data found
else 
  echo '0000000000,0000000000,0000000000,0000000000,0000000000,0000000000,0000000000,0000000000,0000000000,0000000000,0000000000' > .balance_data
  echo copying data && cp .balance_data ~/
fi
